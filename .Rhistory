#PSV
Vmatrix = vcv(NEPphy, corr=F)
psvs = matrix(NA, ncol=2)
index = row.names(Vmatrix) %in% species$X
v.matrix = Vmatrix[index,index]
n = nrow(v.matrix)
psv = (n*sum(diag(v.matrix)) - sum(v.matrix))/(sum(diag(v.matrix))*(n-1))
output = rbind(output, c(i, MRD, psv))
}
